<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app version="3.0"
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

 <servlet>
  <servlet-name>Lista</servlet-name>
  <servlet-class>dron.List</servlet-class>
 </servlet>

 <servlet>
  <servlet-name>Lista2</servlet-name>
  <servlet-class>dron.List2</servlet-class>
 </servlet>

 <servlet-mapping>
  <servlet-name>Lista</servlet-name>
  <url-pattern>/lista</url-pattern>
 </servlet-mapping>

 <servlet-mapping>
  <servlet-name>Lista2</servlet-name>
  <url-pattern>/lista2</url-pattern>
 </servlet-mapping>
</web-app>
